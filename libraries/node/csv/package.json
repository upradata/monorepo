{
    "name": "@upradata/csv",
    "author": {
        "name": "El Pupi (Upra-Data)",
        "email": "elpupi@upradata.com",
        "url": "https://github.com/upradata"
    },
    "repository": {
        "url": "https://github.com/upradata/csv.git"
    },
    "license": "MIT",
    "version": "2.0.0",
    "description": "Some utilities to handle csv",
    "sideEffects": false,
    "main": "lib",
    "types": "lib-esm",
    "module": "lib-esm",
    "files": [
        "lib",
        "lib-esm"
    ],
    "esm": {
        "mainFields": [
            "module",
            "main"
        ]
    },
    "exports-bak": {
        ".": {
            "import": "./lib-esm/index.js",
            "require": "./lib/index.js"
        },
        "./*": {
            "import": "./lib-esm/*",
            "require": "./lib/*"
        }
    },
    "scripts": {
        "clean": "shx rm -rf lib lib-esm",
        "pre:build2": "npm run clean",
        "build2": "concurrently 'tsc -p tsconfig.lib.json' 'tsc -p tsconfig.lib-esm.json'",
        "build": "concurrently 'tsc -p tsconfig.lib.json' 'tsc -p tsconfig.lib-esm.json'",
        "post:build2": "npm run test && npm run e2e",
        "watch": "npm run build:watch",
        "build:watch": "concurrently 'tsc -p tsconfig.lib.json -w' 'tsc -p tsconfig.lib-esm.json -w' 'pnpm run copy:declaration'",
        "test": "jest --verbose false --config jest-ut.config.js",
        "e2e": "jest --passWithNoTests --verbose false --config jest-e2e.config.js",
        "github-push": "npm version patch && git pushall && git pushall-tags",
        "npm-publish": "npm run build && npm publish --access public"
    },
    "dependencies": {
        "csvtojson": "^2.0.10",
        "fs-extra": "^11.1.1",
        "@upradata/util": "workspace:*",
        "@upradata/template-string-style": "workspace:*",
        "@upradata/module": "workspace:*",
        "@upradata/node-util": "workspace:*",
        "@upradata/fs": "workspace:*"
    },
    "devDependencies": {
        "@rushstack/heft": "0.62.3",
        "@upradata/eslint-config": "workspace:*",
        "@upradata/heft-node-rig": "workspace:*",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.0.0",
        "eslint-define-config": "~1.24.1",
        "eslint-import-resolver-exports": "1.0.0-beta.5",
        "eslint-import-resolver-typescript": "~3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-promise": "~6.1.1",
        "eslint-plugin-react": "~7.33.2",
        "eslint-plugin-tsdoc": "~0.2.17",
        "@types/fs-extra": "^11.0.2",
        "@types/jest": "^29.5.5",
        "@types/node": "20.8.4",
        "concurrently": "^8.2.1",
        "jest": "29.7.0",
        "jest-environment-node": "29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "~5.2.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "peersDependencies": {}
}
